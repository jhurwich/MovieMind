<h1>Editing movie</h1>

<%= @movie.title %>

<% form_for(@movie) do |f| %>
  <%= f.error_messages %>

  <!-- Genre -->
  <p>
    <%= render :partial => "genres/genre_selector",
               :locals => {:object => "movie"}%>
  </p>

  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Back', @movie %>
